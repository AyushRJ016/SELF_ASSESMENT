#include <bits/stdc++.h>
using namespace std;
void printf(int i,vector<int>&d,int a[],int n)
{
  if(i>=n){for(auto x:d){ cout<<x;}
  if(d.size()==0) cout<<"{}";
  cout<<endl;
  
   return ;   }

d.push_back(a[i]);
printf(i+1,d,a,n);
d.pop_back();
printf(i+1,d,a,n);

}
int main() {
    int a[]={3,1,2};
    int n=3,i=0;
    vector<int>d;
    printf(i,d,a,n);
}

/*output=
312
31
32
3
12
1
2
{}
*/
